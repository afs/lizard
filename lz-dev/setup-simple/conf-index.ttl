# 2 services, POS and PSO
# 2 servers each.

<#index> rdf:type :IndexService ;
    :name          "IndexPOS" ;
    :indexOrder    "POS" ;
    :servers       ( <#index-s1> ) ;
    .

<#index2> rdf:type :IndexService ;
    :name          "IndexPSO" ;
    :indexOrder    "PSO" ;
    :servers       ( <#index-s2> ) ;
    .

# 2 servers
<#index-s1>  a  :IndexServer ;
    :name       "ServerIndex11" ;
	:hostname   "localhost" ;
	:port       4012 ; 
    :data     	"I1" ;
    .

<#index-s2>  a  :IndexServer ;
    :name       "ServerIndex12" ;
	:hostname   "localhost" ;
	:port       4014 ; 
    :data     	"I2" ;
    .

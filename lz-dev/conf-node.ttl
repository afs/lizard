# 1 service, 2 servers.
<#node>     a   :NodeService ;
    :name      "Node" ;
    :servers  ( <#node-1> <#node-2> ) ;
    .

# 2 servers, each with one segment.
<#node-1>  a :NodeServer ;
    :name       "ServerNode1" ;
	:hostname   "localhost" ;
	:port       4011 ; 
    :data     	"N1" ;
    .

<#node-2>  a :NodeServer ;
    :name       "ServerNode2" ;
	:hostname   "localhost" ;
	:port       4013 ; 
    :data     	"N2" ;
    .
